{
  "name": "ipl/html",
  "type": "library",
  "description": "Icinga PHP Library - HTML abstraction layer",
  "license": "MIT",
  "keywords": ["html"],
  "homepage": "https://github.com/Icinga/ipl-html",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=5.6.0",
    "ipl/stdlib": ">=0.5.0",
    "ipl/validator": ">=0.1.0",
    "psr/http-message": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ipl\\Html\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ipl\\Tests\\Html\\": "tests"
    }
  }
}
